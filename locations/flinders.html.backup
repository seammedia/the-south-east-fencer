    /* CSS Custom Properties (Color Tokens) */
    :root {
        --ink: #0f172a;
        --muted: #475569;
        --border: #e5e7eb;
        --accent: #6b8e79;
        --background: #ffffff;
        
        /* Typography */
        --font-system: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
        --line-height-base: 1.6;
        --line-height-relaxed: 1.8;
        --letter-spacing-wide: 0.1em;
        --letter-spacing-wider: 0.15em;
        
        /* Container */
        --container-max-width: 1200px;
        --spacing-xs: 24px;
        --spacing-sm: 32px;
        --spacing-md: 48px;
        --spacing-lg: 60px;
    }

    /* Reset and Base Styles */
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    body {
        font-family: var(--font-system);
        line-height: var(--line-height-base);
        color: var(--muted);
        background-color: var(--background);
    }

    /* Container */
    .container {
        max-width: var(--container-max-width);
        margin: 0 auto;
        padding: 0 20px;
    }

    /* Header Styles */
    .site-header{position:sticky;top:0;background:#fff;border-bottom:1px solid var(--border);z-index:100}
    .nav-container{max-width:1200px;margin:0 auto;padding:14px 20px;display:flex;align-items:center;justify-content:space-between;gap:24px}
    .brand{font-weight:700;text-decoration:none;color:var(--ink)}
    .site-menu{display:flex;gap:28px;align-items:center}
    .nav-link,.nav-link-button{background:none;border:0;cursor:pointer;font:inherit;text-transform:uppercase;letter-spacing:.2em;font-size:.85rem;text-decoration:none;color:var(--ink);padding:0}
    .nav-link:hover,.nav-link:focus-visible,.nav-link-button:hover,.nav-link-button:focus-visible{text-decoration:underline}
    .nav-link[aria-current="page"], .nav-link-button[aria-current="page"]{text-decoration:underline}
    .nav-toggle{display:none}
    
    /* Dropdown */
    .nav-dropdown{position:relative}
    .dropdown-panel{position:absolute;top:100%;left:0;display:none;min-width:220px;background:#fff;border:1px solid var(--border);border-radius:10px;box-shadow:0 8px 20px rgba(0,0,0,.06);padding:8px}
    .nav-dropdown[aria-expanded="true"] .dropdown-panel{display:block}
    .dropdown-link{display:block;padding:10px 12px;color:var(--ink);text-decoration:none}
    .dropdown-link:hover,.dropdown-link:focus-visible{background:#f7f7f7}
    
    @media (max-width:820px){
      .nav-toggle{display:inline-block}
      .site-menu{display:none;flex-direction:column;gap:12px;padding:12px 0}
      .site-menu[aria-expanded="true"]{display:flex}
    }
    [id]{scroll-margin-top:80px} /* offset for sticky header */
    html{scroll-behavior:smooth}

    /* Location Page Styles */
    .section{padding:64px 20px}
    .two-col{max-width:1200px;margin:0 auto;display:grid;grid-template-columns:1fr 1fr;gap:48px;align-items:start}
    .media img{width:100%;height:auto;border-radius:12px;display:block}
    .content{max-width:65ch;color:var(--muted)}
    .content h1{font-size:clamp(28px,4vw,44px);line-height:1.1;color:var(--ink);margin-bottom:24px}
    .content p{font-size:16px;line-height:var(--line-height-relaxed);margin-bottom:20px;overflow-wrap:break-word;hyphens:auto}
    .content ul{margin:24px 0;padding-left:0;list-style:none}
    .content ul li{font-size:16px;margin-bottom:8px;position:relative;padding-left:24px}
    .content ul li::before{content:"✓";position:absolute;left:0;color:var(--accent);font-weight:bold}
    .btn-row{margin-top:32px}
    .btn-row .btn{margin-right:10px}
    .btn{display:inline-block;padding:12px 24px;background:var(--accent);color:#fff;border-radius:8px;text-decoration:none;font-weight:500;transition:filter 0.3s ease}
    .btn:hover,.btn:focus-visible{filter:brightness(.95);outline:2px solid var(--accent);outline-offset:2px}
    @media (max-width:900px){.two-col{grid-template-columns:1fr;gap:24px}}

    /* Footer */
    .footer {
        background-color: #f8f9fa;
        padding: var(--spacing-md) 0;
        border-top: 1px solid var(--border);
    }

    .footer-content {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        gap: var(--spacing-sm);
    }

    .footer-section h4 {
        font-size: 14px;
        font-weight: bold;
        color: var(--ink);
        margin-bottom: 16px;
        letter-spacing: var(--letter-spacing-wide);
        text-transform: uppercase;
    }

    .footer-section ul {
        list-style: none;
    }

    .footer-section ul li {
        margin-bottom: 8px;
    }

    .footer-section a {
        text-decoration: none;
        color: var(--muted);
        font-size: 14px;
        transition: color 0.3s ease;
    }

    .footer-section a:hover,
