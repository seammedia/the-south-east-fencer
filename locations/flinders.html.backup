        color: var(--accent);
    }

    .footer-section p {
        color: var(--muted);
        font-size: 14px;
        margin-bottom: 8px;
    }

    @media (max-width: 768px) {
        .footer-content {
            grid-template-columns: 1fr;
            gap: var(--spacing-sm);
            text-align: center;
        }
    }
  </style>
