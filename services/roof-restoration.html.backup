.btn{display:inline-block;padding:10px 14px;border-radius:10px;background:var(--accent);color:#fff;text-decoration:none;font-weight:500;font-size:.85rem;text-transform:uppercase;letter-spacing:.1em}
.btn-ghost{background:transparent;border:1px solid var(--border);color:var(--ink)}
.nav-toggle{display:none}
.hide-md{display:inline-block}
/* Dropdown */
.nav-dropdown{position:relative}
.dropdown-panel{position:absolute;top:100%;left:0;display:none;min-width:230px;background:#fff;border:1px solid var(--border);border-radius:10px;box-shadow:0 8px 20px rgba(0,0,0,.06);padding:8px}
.nav-dropdown[aria-expanded="true"] .dropdown-panel{display:block}
