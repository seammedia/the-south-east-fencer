    /* CSS Custom Properties (Color Tokens) */
    :root {
        --ink: #0f172a;
        --muted: #475569;
        --border: #e5e7eb;
        --accent: #6b8e79;
        --background: #ffffff;
        
        /* Typography */
        --font-system: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
        --line-height-base: 1.6;
        --line-height-relaxed: 1.8;
        --letter-spacing-wide: 0.1em;
        --letter-spacing-wider: 0.15em;
        
        /* Container */
        --container-max-width: 1200px;
        --spacing-xs: 24px;
        --spacing-sm: 32px;
        --spacing-md: 48px;
        --spacing-lg: 60px;
    }

    /* Reset and Base Styles */
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    body {
        font-family: var(--font-system);
        line-height: var(--line-height-base);
        color: var(--muted);
        background-color: var(--background);
    }

    /* Container */
    .container {
        max-width: var(--container-max-width);
        margin: 0 auto;
        padding: 0 20px;
    }

    /* Header Styles */
    .site-header{position:sticky;top:0;background:#fff;border-bottom:1px solid var(--border);z-index:100}
    .nav-container{max-width:1200px;margin:0 auto;padding:14px 20px;display:flex;align-items:center;justify-content:space-between;gap:24px}
    .brand{font-weight:700;text-decoration:none;color:var(--ink)}
    .site-menu{display:flex;gap:22px;align-items:center;flex-wrap:wrap}
    .nav-link,.nav-link-button{background:none;border:0;cursor:pointer;font:inherit;text-transform:uppercase;letter-spacing:.2em;font-size:.85rem;text-decoration:none;color:var(--ink);padding:0}
    .nav-link:hover,.nav-link:focus-visible,.nav-link-button:hover,.nav-link-button:focus-visible{text-decoration:underline}
    .btn{display:inline-block;padding:10px 14px;border-radius:10px;background:var(--accent);color:#fff;text-decoration:none;font-weight:500;font-size:.85rem;text-transform:uppercase;letter-spacing:.1em}
    .btn-ghost{background:transparent;border:1px solid var(--border);color:var(--ink)}
    .nav-toggle{display:none}
    .hide-md{display:inline-block}
    /* Dropdown */
    .nav-dropdown{position:relative}
    .dropdown-panel{position:absolute;top:100%;left:0;display:none;min-width:220px;background:#fff;border:1px solid var(--border);border-radius:10px;box-shadow:0 8px 20px rgba(0,0,0,.06);padding:8px}
    .nav-dropdown[aria-expanded="true"] .dropdown-panel{display:block}
    .dropdown-link{display:block;padding:10px 12px;color:var(--ink);text-decoration:none}
    .dropdown-link:hover,.dropdown-link:focus-visible{background:#f7f7f7}
    /* Active state */
    .nav-link[aria-current="page"], .nav-link-button[aria-current="page"]{text-decoration:underline}
    /* Fit on medium screens */
    @media (max-width:1024px){
      .site-menu{gap:16px}
      .nav-link,.nav-link-button{letter-spacing:.16em;font-size:.82rem}
      .hide-md{display:none}
    }
    /* Mobile menu */
    @media (max-width:820px){
      .nav-toggle{display:inline-block}
      .site-menu{display:none;flex-direction:column;gap:12px;padding:12px 0}
      .site-menu[aria-expanded="true"]{display:flex}
    }
    /* Anchor offset + smooth scroll */
    [id]{scroll-margin-top:80px}
    html{scroll-behavior:smooth}

    /* Blog Styles */
    .blog-hero{padding:48px 20px 16px}
    .blog-wrap{max-width:1200px;margin:0 auto}
    .blog-title{font-size:clamp(28px,4vw,48px);line-height:1.1;color:var(--ink)}
    .grid{display:grid;grid-template-columns:repeat(3,1fr);gap:28px;padding:24px 20px 60px}
    @media (max-width:1024px){.grid{grid-template-columns:repeat(2,1fr)}}
    @media (max-width:680px){.grid{grid-template-columns:1fr}}
    .card{border:1px solid var(--border);border-radius:14px;overflow:hidden;background:#fff;box-shadow:0 2px 8px rgba(0,0,0,.04);transition:transform 0.2s ease,box-shadow 0.2s ease}
    .card:hover{transform:translateY(-2px);box-shadow:0 8px 25px rgba(0,0,0,.1)}
    .card-media{aspect-ratio:16/9;background:#f5f5f5}
    .card-media img{width:100%;height:100%;object-fit:cover;display:block}
    .card-body{padding:16px 18px 14px}
    .tag{display:inline-block;font-size:.75rem;padding:6px 10px;border-radius:999px;background:#f0f0f0;color:var(--muted);text-transform:uppercase;letter-spacing:.05em}
    .card h3{font-size:1.1rem;line-height:1.3;margin:6px 0 8px;color:var(--ink)}
    .card p{color:var(--muted);margin:0 0 12px}
    .meta{font-size:.8rem;color:#7a7a7a;border-top:1px solid var(--border);padding:10px 18px}
    .card-link{display:block;text-decoration:none;color:inherit}
    .card-link .card{transition:transform .12s ease, box-shadow .12s ease}
    .card-link:hover .card, .card-link:focus-visible .card{
      transform:translateY(-2px);
      box-shadow:0 10px 28px rgba(0,0,0,.08);
    }
    .card h3{color:var(--ink)}

    /* Footer */
    .footer {
        background-color: #f8f9fa;
        padding: var(--spacing-md) 0;
        border-top: 1px solid var(--border);
    }

    .footer-content {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        gap: var(--spacing-sm);
    }

    .footer-section h4 {
        font-size: 14px;
        font-weight: bold;
        color: var(--ink);
        margin-bottom: 16px;
        letter-spacing: var(--letter-spacing-wide);
        text-transform: uppercase;
    }

    .footer-section ul {
        list-style: none;
    }

    .footer-section ul li {
        margin-bottom: 8px;
    }

    .footer-section a {
        text-decoration: none;
        color: var(--muted);
        font-size: 14px;
        transition: color 0.3s ease;
    }

    .footer-section a:hover,
    .footer-section a:focus {
        color: var(--accent);
    }

    .footer-section p {
        color: var(--muted);
        font-size: 14px;
        margin-bottom: 8px;
    }

    @media (max-width: 768px) {
        .footer-content {
            grid-template-columns: 1fr;
            gap: var(--spacing-sm);
            text-align: center;
        }
    }

    /* Two-column layout for locations list in footer */
    .footer-links.loc-columns {
        list-style: none;
        margin: 0;
        padding: 0;
    }

    .footer-links.loc-columns li {
        margin-bottom: 8px;
        break-inside: avoid;
    }

    @media (min-width: 768px) {
        .footer-links.loc-columns {
            columns: 2;
            column-gap: 24px;
        }
    }

    @media (max-width: 767px) {
        .footer-links.loc-columns {
            columns: 1;
        }
    }
  </style>
